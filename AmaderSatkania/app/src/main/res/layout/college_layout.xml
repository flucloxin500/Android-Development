            android:layout_marginRight="10sp"
            android:layout_marginLeft="15sp"
            />


    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <soup.neumorphism.NeumorphCardView
            android:id="@+id/neumorphCardView5"
            android:layout_width="300dp"
            android:layout_height="85dp"
            app:neumorph_shadowColorDark="@color/thankYou_des"
            app:neumorph_shadowColorLight="@color/text_color2"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="0dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"

            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="কর্নেল (অবঃ) অলি আহমদ বীর বিক্রম কলেজ"
                android:textSize="18sp"
                android:layout_gravity="center"
                android:textColor="#1a2421"
                android:layout_marginLeft="10dp"
                />

        </soup.neumorphism.NeumorphCardView>

        <Button
            android:id="@+id/bcorcolloc"
            android:layout_width="50dp"
